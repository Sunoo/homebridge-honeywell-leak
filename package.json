{
  "displayName": "Homebridge Honeywell Leak",
  "name": "homebridge-honeywell-leak",
  "version": "1.2.0",
  "description": "Honeywell Weak Leak Detector / Resideo Droplet plugin for Homebridge.",
  "author": "David Maher",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sunoo/homebridge-honeywell-leak.git"
  },
  "bugs": {
    "url": "https://github.com/Sunoo/homebridge-honeywell-leak/issues"
  },
  "homepage": "https://github.com/Sunoo/homebridge-honeywell-leak#readme",
  "engines": {
    "node": ">=12.0.0",
    "homebridge": ">=1.1.2"
  },
  "main": "dist/index.js",
  "scripts": {
    "format": "markdownlint --fix *.md",
    "developer": "DEBUG=honeywell-home homebridge -D -U `pwd` -P `pwd`",
    "lint": "eslint src/**.ts --fix",
    "lint-check": "eslint src/*.ts",
    "watch": "npm run build && npm link && nodemon",
    "clean": "rimraf ./dist ./coverage",
    "build": "rimraf ./dist ./coverage && tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "homebridge-plugin",
    "honeywell",
    "leak",
    "leak detector",
    "water leak detector",
    "resideo",
    "droplet"
  ],
  "files": [
    "config.schema.json",
    "dist/**/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "homebridge": "^1.1.2",
    "markdownlint-cli": "^0.23.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "typescript-axios-wb": "^1.0.3"
  }
}
